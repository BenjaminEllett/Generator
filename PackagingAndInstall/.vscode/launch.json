    "version": "0.2.0",
    "configurations": [

        {
            "name": "Install both Generator versions",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/InstallGenerator.ps1",
            "args": [
                "-InstallCommandLineVersion"
            ]
        },

        {
            "name": "Add Generator to user's path",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/AddGeneratorToUsersPath.ps1",
        },

        {
            "name": "Uninstall Generator",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/UninstallGenerator.ps1",
        },

        {
            "name": "Create Generator Release Package (ZIP Files)",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/CreateReleaseZipFiles.ps1",
        },

        {
            "name": "Create Generator Install Script Package",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/CreateInstallProgramZipFile.ps1",
        },

        {
            "name": "Upload release to Azure storage account",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/UploadReleaseToAzureStorageAccount.ps1",
        },
    ]
}